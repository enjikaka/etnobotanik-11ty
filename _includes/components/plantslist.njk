<nav>
  {% set plantPages = collections.plant %}
  <ul class="plants-list">
  {%- for plantPage in plantPages %}
    {%- if plantPage.data.latinName %}
      {% set imageUrl = plantPage.data.featured_image | default('/static/img/fallback-plant.svg') %}
      <li>
        <a href="{{ plantPage.url | url }}">
          <figure>
            <img src="{{imageUrl}}" alt="Bild på {{ plantPage.data.title }}">
          </figure>
          {{ plantPage.data.title }} ({{ plantPage.data.latinName }})
        </a>
      </li>
    {%- endif %}
  {%- endfor %}
  </ul>
</nav>
