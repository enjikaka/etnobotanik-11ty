<nav>
  <ul class="plants-list">
  {% set css %}
			{% include "assets/css/plants-list.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {%- for plantPage in plantPages %}
    {%- if plantPage.data.latinName %}
      <li>
        <a href="{{ plantPage.url | url }}">
          {% image plantPage.data.latinName | thumbnailOriginalSrc, "Utvald bild f√∂r " + plantPage.data.title, "64px" %}
          <div class="text">
            <span>{{ plantPage.data.title }}</span>
            <small>{{ plantPage.data.latinName }}</small>
          </div>
        </a>
      </li>
    {%- endif %}
  {%- endfor %}
  </ul>
</nav>
