<nav>
  <ul class="plants-list">
  {%- for plantPage in plantPages %}
    {%- if plantPage.data.latinName %}
      <li>
        <a href="{{ plantPage.url | url }}">
          <figure>
            <img
              src="{{plantPage.data.latinName | thumbnailSrc}}"
              srcset="{{ plantPage.data.latinName | thumbnailSrcset }}"
              sizes="64px" alt="Bild pÃ¥ {{ plantPage.data.title }}"
              decoding="async"
              loading="lazy"
            >
          </figure>
          {{ plantPage.data.title }} ({{ plantPage.data.latinName }})
        </a>
      </li>
    {%- endif %}
  {%- endfor %}
  </ul>
</nav>
